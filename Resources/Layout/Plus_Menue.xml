<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#f1e7e6"
    android:id="@+id/lRelLayout23">
    <LinearLayout
        android:id="@+id/Text12"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#8d6a8b">
        <EditText
            android:id="@+id/Text1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="#ffffffff"
            android:textSize="20dip"
            android:textStyle="bold"
            android:hint="Titel hinzufügen ..."
            android:textColorHint="#ffbdbdbd"
            android:inputType="text"
            android:maxLines="1"
            android:backgroundTint="@android:color/transparent" />
        <EditText
            android:id="@+id/Text2"
            android:inputType="textMultiLine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14dip"
            android:textColor="#ffffffff"
            android:textStyle="normal"
            android:hint="Beschreibung hinzufügen ..."
            android:textColorHint="#ffbdbdbd"
            android:minHeight="75dp"
            android:maxHeight="75dp"
            android:gravity="center_vertical"
            android:maxLines="3"
            android:backgroundTint="@android:color/transparent" />
    </LinearLayout>
    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/linearLayout13"
        android:layout_below="@+id/RelLayoutdropplazieren"
        android:layout_above="@+id/textView35"
        android:scaleType="fitCenter"
        android:background="#ff616161"
        android:src="@android:drawable/ic_menu_gallery" />
    <TextView
        android:text="Small Text"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView35"
        android:layout_above="@+id/relativeLayout_navbar2"
        android:textColor="@android:color/transparent" />
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout13"
        android:layout_below="@+id/RelLayoutdropplazieren"
        android:layout_above="@id/relativeLayout_navbar2">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/linearLayout12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:id="@+id/LinearFileFotoPick"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/button1"
                    android:alpha="0" />
                <ImageButton
                    android:src="@drawable/aufmap_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton1"
                    android:background="@android:color/transparent" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/button2"
                    android:alpha="0" />
                <TextView
                    android:id="@+id/Text3"
                    android:text="Bild hinzufügen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14dip"
                    android:textColor="#807c7b"
                    android:textStyle="normal"
                    android:layout_alignTop="@+id/imageButton1"
                    android:layout_alignBottom="@+id/imageButton1"
                    android:gravity="center" />
                <Button
                    android:text="Button"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/button3"
                    android:background="@android:color/transparent"
                    android:alpha="0" />
                <Button
                    android:text="Button"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/button3"
                    android:background="@android:color/transparent"
                    android:alpha="0" />
            </LinearLayout>
            <ImageButton
                android:src="@android:drawable/ic_menu_gallery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton569"
                android:background="@android:color/transparent"
                android:alpha="0" />
        </LinearLayout>
    </LinearLayout>
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:id="@+id/relLayout2"
        android:layout_below="@+id/DateAndTime">
        <ImageButton
            android:src="@drawable/icon_hap1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton33"
            android:background="@android:color/transparent"
            android:layout_toRightOf="@+id/imageButton55"
            android:layout_centerVertical="true" />
        <ImageButton
            android:src="@drawable/icon_hap2_off"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton44"
            android:background="@android:color/transparent"
            android:layout_toRightOf="@+id/imageButton33"
            android:layout_centerVertical="true" />
        <ImageButton
            android:src="@drawable/icon_hap3_off"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton55"
            android:background="@android:color/transparent"
            android:layout_toRightOf="@+id/button1"
            android:layout_centerVertical="true" />
        <TextView
            android:text="Event"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_toRightOf="@+id/imageButton44"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:gravity="center"
            android:textColor="#807c7b"
            android:textStyle="bold" />
        <Button
            android:id="@+id/button1"
            android:alpha="0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/relativeLayout_navbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_alignParentBottom="true"
        android:background="#c25e5c">
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/linearLayout11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true">
            <ImageButton
                android:src="@android:color/transparent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageButton567"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:background="@android:color/transparent" />
            <ImageButton
                android:src="@drawable/kreuz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton367"
                android:background="@android:color/transparent"
                android:scaleType="fitCenter" />
            <ImageButton
                android:src="@drawable/haken"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton467"
                android:background="@android:color/transparent"
                android:scaleType="fitCenter" />
        </LinearLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/DateAndTime"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:layout_below="@+id/Text12">
        <ImageButton
            android:src="@android:drawable/ic_menu_gallery"
            android:id="@+id/imageButton572"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:alpha="0"
            android:layout_marginBottom="0.0dp" />
        <LinearLayout
            android:id="@+id/ganztaegig"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:layout_below="@+id/imageView1">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/button1"
                android:alpha="0" />
            <ImageView
                android:src="@drawable/wann_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                android:layout_weight="0.1" />
            <TextView
                android:id="@+id/Text46"
                android:text="Ganztägig"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#807c7b"
                android:textStyle="normal"
                android:gravity="center"
                android:layout_weight="0.1" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/schiebtSwitchRechts"
                android:layout_weight="0.65" />
            <Switch
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/switch_button"
                android:layout_weight="0.15"
                android:visibility="visible"
                android:thumb="@drawable/switch1"
                android:track="@drawable/switch2" />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:id="@+id/button1"
                android:alpha="0" />
        </LinearLayout>
        <TextView
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView34" />
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout15">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:id="@+id/linearLayout14"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.8">
                <TextView
                    android:text="Von dd.mm.yyyy"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView39"
                    android:gravity="center"
                    android:textColor="#807c7b" />
                <DatePicker xmlns:p3="http://schemas.android.com/tools"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/datePicker1"
                    p3:visibility="gone"
                    style="@android:style/Widget.Material.Light.DatePicker" />
                <TextView
                    android:text="Small Text"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView36"
                    android:textColor="@android:color/transparent" />
                <TextView
                    android:text="Bis dd.mm.yyyy"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView29"
                    android:textColor="#807c7b"
                    android:gravity="center" />
                <DatePicker xmlns:tools="http://schemas.android.com/tools"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/datePicker2"
                    tools:visibility="gone" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:id="@+id/linearLayout143"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.2">
                <TextView
                    android:text="15:00"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView33"
                    android:textColor="#807c7b" />
                <TextView
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView36"
                    android:textColor="@android:color/transparent" />
                <TextView
                    android:text="15:00"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView22"
                    android:textColor="#807c7b" />
            </LinearLayout>
        </LinearLayout>
        <ImageButton
            android:src="@android:drawable/ic_menu_gallery"
            android:id="@+id/imageButton572"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:alpha="0"
            android:layout_marginBottom="0.0dp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/RelLayoutdropplazieren"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relLayout2"
        android:minWidth="25px"
        android:minHeight="25px">
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:id="@+id/linearLayout12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageButton
                android:src="@android:drawable/ic_menu_gallery"
                android:id="@+id/imageButton568"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:alpha="0" />
            <LinearLayout
                android:id="@+id/LinearFileFotoPick2"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/button1"
                    android:alpha="0" />
                <ImageButton
                    android:src="@drawable/aufmap_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton1"
                    android:background="@android:color/transparent" />
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/button2"
                    android:alpha="0" />
                <TextView
                    android:id="@+id/Text3"
                    android:text="Auf Karte platzieren"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14dip"
                    android:textColor="#807c7b"
                    android:textStyle="normal"
                    android:layout_alignTop="@+id/imageButton1"
                    android:layout_alignBottom="@+id/imageButton1"
                    android:gravity="center" />
                <Button
                    android:text="Button"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/button3"
                    android:background="@android:color/transparent"
                    android:alpha="0" />
            </LinearLayout>
            <ImageButton
                android:src="@android:drawable/ic_menu_gallery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton569"
                android:background="@android:color/transparent"
                android:alpha="0" />
        </LinearLayout>
    </LinearLayout>
    <Button
        android:text="Button"
        android:id="@+id/button3"
        android:background="@android:color/transparent"
        android:alpha="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>